{
  "name": "angular-skeleton",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:ci": "ng build --prod --base-href / --deploy-url /",
    "test": "npx jest",
    "test:watch": "npx jest --watch",
    "test:ci": "npx jest --ci --coverage --reporters=default --reporters=jest-junit --watchAll=false",
    "eslint": "eslint \"src/**/*.ts\"",
    "lint": "npm run eslint && npm run lint:styles",
    "lint:fix": "npm run eslint -- --fix && npm run lint:stylesFix",
    "lint:styles": "stylelint \"src/app/**/*.scss\"",
    "lint:stylesFix": "stylelint \"src/app/**/*.scss\" --fix",
    "cypress:open": "npx cypress open",
    "cypress:run": "npx cypress run",
    "e2e": "npm run cypress:open",
    "e2e:ci": "npm run cypress:run"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.0.1",
    "@angular/common": "~11.0.1",
    "@angular/compiler": "~11.0.1",
    "@angular/core": "~11.0.1",
    "@angular/forms": "~11.0.1",
    "@angular/platform-browser": "~11.0.1",
    "@angular/platform-browser-dynamic": "~11.0.1",
    "@angular/router": "~11.0.1",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1100.2",
    "@angular-eslint/builder": "1.2.0",
    "@angular-eslint/eslint-plugin": "1.2.0",
    "@angular-eslint/eslint-plugin-template": "1.2.0",
    "@angular-eslint/schematics": "1.2.0",
    "@angular-eslint/template-parser": "1.2.0",
    "@angular/cli": "~11.0.2",
    "@angular/compiler-cli": "~11.0.1",
    "@types/jasmine": "~3.6.0",
    "@types/jest": "^27.0.2",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "4.3.0",
    "@typescript-eslint/parser": "4.3.0",
    "codelyzer": "^6.0.0",
    "cypress": "^8.6.0",
    "eslint": "^7.6.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "30.7.6",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "jasmine-core": "~3.8.0",
    "jasmine-spec-reporter": "~5.0.0",
    "jest": "^27.3.1",
    "jest-junit": "^13.0.0",
    "jest-preset-angular": "^10.0.1",
    "postcss-scss": "^4.0.1",
    "stylelint": "^14.0.0",
    "stylelint-config-standard-scss": "^2.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "tsutils": "^3.21.0",
    "typescript": "~4.0.2"
  }
}
